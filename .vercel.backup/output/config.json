{
  "version": 3,
  "routes": [
    {
      "src": "^/api(?:/(.*))$",
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "GET, POST, PUT, DELETE, OPTIONS",
        "Access-Control-Allow-Headers": "Content-Type, Authorization"
      },
      "continue": true
    },
    {
      "src": "^(?:/(version|releases))\\.json$",
      "headers": {
        "Cache-Control": "no-store, no-cache, must-revalidate",
        "Content-Type": "application/json",
        "Access-Control-Allow-Origin": "*"
      },
      "continue": true
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "^/api(?:/(.*))$",
      "dest": "/api/$1",
      "check": true
    },
    {
      "src": "^(?:/((?!api|.*\\.\\w+).*))$",
      "dest": "/index.html",
      "check": true
    },
    {
      "handle": "error"
    },
    {
      "status": 404,
      "src": "^(?!/api).*$",
      "dest": "/404.html"
    }
  ],
  "framework": {
    "version": "5.4.19"
  },
  "crons": []
}
